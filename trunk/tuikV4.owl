      <Stage1 rdf:ID="Stage1_1">
        <ilkogretimOgrenciSayisi>
          <Stage1Value rdf:ID="Stage1Value_8"/>
        </ilkogretimOgrenciSayisi>
        <hasStage2>
          <Stage2 rdf:ID="Stage2_1">
            <ilkogretimOgrenciSayisi>
              <Stage2Value rdf:ID="Stage2Value_9"/>
            </ilkogretimOgrenciSayisi>
            <hasCity>
              <City rdf:ID="Bursa">
                <hasValue>
