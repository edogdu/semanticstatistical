                </hasValue>
                <ilkogretimOgrenciSayisi rdf:resource="#CityValue_7"/>
                <hasStage2 rdf:resource="#Stage2_1"/>
              </City>
            </hasCity>
            <hasValue rdf:resource="#Stage2Value_9"/>
            <hasRegion rdf:resource="#Stage1_1"/>
          </Stage2>
        </hasStage2>
      </Stage1>
    </hasRegion>
    <hasValue rdf:resource="#TurkeyValue_10"/>
  </Turkey>
